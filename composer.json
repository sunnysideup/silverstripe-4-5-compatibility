{
    "name": "sunnysideup/silverstripe-4-5-compatibility",
    "description": "Allow Silverstripe 4 modules to work with Silverstripe 5 and vice versa.",
    "type": "silverstripe-vendormodule",
    "keywords": [
        "Silverstripe-CMS",
        "CMS",
        "Silverstripe",
        "Silverstripe 4.0",
        "Silverstripe 5.0",
        "compatibility",
        "upgrade"
    ],
    "require": {
        "silverstripe/cms": "4.*",
        "symbiote/silverstripe-grouped-cms-menu": "*",
        "undefinedoffset/sortablegridfield": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Nicolaas Francken",
            "email": "modules@sunnysideup.co.nz",
            "role": "lead developer"
        }
    ],
    "extra": {
        "expose": [
            "client/dist"
        ]
    },
    "autoload": {
        "psr-4": {
            "Sunnysideup\\Silverstripe4To5Compatibility\\": "src/"
        }
    }
}
